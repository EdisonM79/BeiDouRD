<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="SessionReplyListView">
        <attr name="replierAndTargetTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ExpandTextView">
        <attr name="defaultShownLines" format="integer" />
        <attr name="foldableTriggerTextColor" format="color" />
        <attr name="foldStateTriggerText" format="string" />
        <attr name="unfoldStateTriggerText" format="string" />
    </declare-styleable>

    <declare-styleable name="ChatView">
        <attr name="msgColor" format="color" />
        <!--<attr name="send"-->
        <attr name="textSize" format="dimension" />
        <attr name="mode">
            <enum name="comment" value="0" />
            <enum name="chat" value="1" />
        </attr>
        <attr name="sendBtnBg" format="reference" />
        <attr name="sendBtnTextColor" format="color" />
    </declare-styleable>


    <declare-styleable name="RatingView">
        <attr name="starsNum" format="integer" />
        <attr name="starWidth" format="dimension" />
        <attr name="starHeight" format="dimension" />
        <attr name="starDistance" format="dimension" />
        <attr name="starBackground" format="reference" />
        <attr name="starDrawBackground" format="reference" />
        <attr name="isIndicator" format="boolean" />
        <!--默认星星数(可为小数)，取值范围[0-starNum]-->
        <attr name="defaultStarNum" format="float" />
        <!--每颗星星代表的分值-->
        <attr name="valuePerStar" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomRadioGroup">
        <attr name="orientation">
            <enum name="Horizontal" value="0" />
            <enum name="Vertical" value="1" />
        </attr>
        <attr name="checkedButton" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircleCountDownView">
        <!--控件中间图片资源-->
        <attr name="circleImgSrc" format="reference" />
        <attr name="circleBorderColor" format="color" />
        <attr name="circleBorderWidth" format="dimension" />
        <attr name="valueTextSize" format="dimension" />
        <attr name="valueTextColor" format="color" />
        <attr name="padding" format="dimension" />
        <attr name="showProgress" format="boolean" />
        <attr name="processColorStart" format="color" />
        <attr name="processColorEnd" format="color" />
        <attr name="processBlurMaskRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MultiGroupHistogramView">
        <attr name="coordinateAxisWidth" format="dimension" />
        <attr name="coordinateAxisColor" format="color" />
        <attr name="groupNameTextColor" format="color" />
        <attr name="groupNameTextSize" format="dimension" />
        <attr name="groupInterval" format="dimension" />
        <attr name="histogramInterval" format="dimension" />
        <attr name="histogramValueTextColor" format="color" />
        <attr name="histogramValueTextSize" format="dimension" />
        <attr name="histogramHistogramWidth" format="dimension" />
        <attr name="histogramPaddingStart" format="dimension" />
        <attr name="histogramPaddingEnd" format="dimension" />
        <attr name="chartPaddingTop" format="dimension" />
        <attr name="distanceFormGroupNameToAxis" format="dimension" />
        <attr name="distanceFromValueToHistogram" format="dimension" />
        <!--图表数值小数点位数-->
        <attr name="histogramValueDecimalCount">
            <enum name="ZERO" value="0" />
            <enum name="ONE" value="1" />
            <enum name="TWO" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomIndicator">
        <attr name="indicatorType" format="enum">
            <enum name="scale" value="0" />
            <enum name="gradual" value="1" />
            <enum name="split" value="2" />
            <enum name="scaleAndGradual" value="3" />
        </attr>
        <attr name="normalPointRadius" format="dimension" />
        <attr name="selectedPointRadius" format="dimension" />
        <attr name="normalPointColor" format="color" />
        <attr name="selectedPointColor" format="color" />
        <attr name="pointInterval" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AudioSignalView">
        <attr name="asv_groupCount" format="integer" />
        <attr name="asv_groupInterval" format="dimension" />
        <attr name="asv_lineColor" format="color" />
        <attr name="asv_lineWidth" format="dimension" />
        <attr name="asv_lineInterval" format="dimension" />
        <attr name="asv_maxDynamicLineHeight" format="dimension" />
        <attr name="asv_maxStaticLineHeight" format="dimension" />
        <attr name="asv_minStaticLineHeight" format="dimension"/>
        <attr name="asv_lineCountInGroup" format="integer"/>
    </declare-styleable>
</resources>